        </activity>
        <activity
            android:name=".Main2Activity"
            android:label=""
            >
        <intent-filter>
            <action android:name="com.example.nikola.Main2Activity" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>
        <activity android:name=".Main3Activity">
            <intent-filter>
                <action android:name="com.example.nikola.Main3Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>